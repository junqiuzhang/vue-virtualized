{
  "name": "vue-virtualized",
  "version": "1.0.0",
  "description": "vue virtualized render",
  "files": [
    "dist"
  ],
  "main": "./dist/vue-virtualized.umd.js",
  "module": "./dist/vue-virtualized.es.js",
  "exports": {
    ".": {
      "import": "./dist/vue-virtualized.es.js",
      "require": "./dist/vue-virtualized.umd.js"
    }
