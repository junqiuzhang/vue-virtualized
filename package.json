  "bugs": {
    "url": "https://github.com/junqiuzhang/vue-window/issues"
  },
  "homepage": "https://github.com/junqiuzhang/vue-window#readme",
  "devDependencies": {
    "@vitejs/plugin-vue": "^1.2.3",
    "@vitejs/plugin-vue-jsx": "^1.1.5",
    "typescript": "^4.3.2",
    "vite": "^2.3.7",
    "vue-tsc": "^0.1.7"
  },
  "dependencies": {
    "vue": "3.x.x"
  }
}
