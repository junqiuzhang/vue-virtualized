{
  "name": "vue-virtualized",
  "version": "1.4.8",
  "description": "vue virtualized render",
  "files": [
    "dist"
  ],
  "main": "./dist/vue-virtualized.umd.js",
  "module": "./dist/vue-virtualized.es.js",
  "exports": {
    ".": {
      "import": "./dist/vue-virtualized.es.js",
      "require": "./dist/vue-virtualized.umd.js"
    }
  },
  "type": "module",
  "types": "./dist/vue-virtualized.d.ts",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "serve": "vite preview"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/junqiuzhang/vue-virtualized.git"
  },
  "author": "junqiuzhang",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/junqiuzhang/vue-virtualized/issues"
  },
  "homepage": "https://github.com/junqiuzhang/vue-virtualized#readme",
  "keywords": [
    "vue",
    "vuejs",
    "virtual",
    "virtualized",
    "list",
    "scrolling"
  ],
  "devDependencies": {
    "@vitejs/plugin-vue": "^1.2.3",
    "@vitejs/plugin-vue-jsx": "^1.1.5",
    "typescript": "^4.3.2",
    "vite": "^2.3.7",
    "vue-tsc": "^0.1.7"
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  }
}
